{
  "name": "@zajno/static-astro-site-template",
  "version": "1.1.2",
  "description": "Static website astro template by Zajno",
  "private": true,
  "author": "Zajno <hello@zajno.com> (https://zajno.com)",
  "type": "module",
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "yarn build:ts && astro build",
    "build:ts": "yarn tsc --noEmit",
    "build:prod": "yarn build --define-process-env-node-env production",
    "build:release:staging": "yarn && (cross-env APP_ENV=staging yarn build:prod)",
    "build:release:production": "yarn && (cross-env APP_ENV=production yarn build:prod)",
    "preview": "astro preview",
    "astro": "astro",
    "emulate": "firebase emulators:start --only hosting",
    "lint": "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "@astrojs/sitemap": "^1.0.0",
    "@zajno/common": "^2.0.5",
    "@zajno/common-web": "^1.0.0",
    "astro": "^2.3.4",
    "detect-browser": "^5.3.0",
    "gsap": "npm:@gsap/shockingly",
    "lottie-web": "^5.10.0",
    "regenerator-runtime": "^0.13.11"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@zajno/eslint-config": "^2.3.0",
    "astro-compress": "^1.1.18",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-plugin-astro": "^0.21.0",
    "git-rev-sync": "^3.0.2",
    "sass": "^1.56.1",
    "typescript": "^5.0.4"
  }
}
