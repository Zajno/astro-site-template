---
import PageLayot from 'layouts/page.astro';
import { NoScript as PageSettings } from 'sitemap/pages';

import { type Locales } from 'sitemap/copyright';
import MainSection from 'sections/noScript/main/view.astro';

type Props = {
	locale: Locales
};

const { locale = 'en' } = Astro.props;
---

<PageLayot
    {...PageSettings}
    locale={locale}
    isActiveHeader={false}
    isActiveFooter={false}
    isActiveMobileMenu={false}
    isActivePreloader={false}
>
    <MainSection />
</PageLayot>
