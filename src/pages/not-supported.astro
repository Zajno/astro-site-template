---
import PageLayot from 'layouts/page.astro';
import { NotSupported as PageSettings} from 'sitemap/pages';
import { Locales } from 'sitemap/copyright';
import MainSection from 'sections/notSupported/main/view.astro';

type Props = {
	locale: Locales
}

const {locale = 'en'} = Astro.props;

---

<PageLayot
    {...PageSettings}
    locale={locale}
    isActiveHeader={false}
    isActiveFooter={false}
    isActiveMobileMenu={false}
>
    <MainSection />
</PageLayot>
